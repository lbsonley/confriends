{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 7
	},
	"extends": [
		"airbnb",
		"prettier"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"prettier/prettier": ["error", {"singleQuote": true, "trailingComma": "all"}]
	},
	"env": {
		"browser": true,
		"node": true
	}
}